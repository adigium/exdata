    "inversify": "^6.0.2",
    "inversify-inject-decorators": "^3.1.0",
    "lodash": "^4.17.21",
    "mongoose": "^8.1.1",
    "p-limit": "^3.1.0",
    "public-ip": "^4.0.4",
    "reflect-metadata": "^0.2.2",
